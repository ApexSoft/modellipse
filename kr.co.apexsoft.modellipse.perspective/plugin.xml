<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.4"?><plugin>
   <extension point="org.eclipse.ui.perspectives">
      <perspective class="kr.co.apexsoft.modellipse.perspective.ModellipsePerspective" icon="icons/Logo-16-8bit.bmp" id="kr.co.apexsoft.modellipse.perspective.perspective" name="Modellipse">
      </perspective>
   </extension>
   <extension id="product" point="org.eclipse.core.runtime.products">
      <product application="org.eclipse.ui.ide.workbench" name="Modellipse">
         <property name="appName" value="Modellipse">
         </property>
         <property name="windowImages" value="icons/Logo-windowImage-16.png">
         </property>
         <property name="startupProgressRect" value="5,280,445,15">
         </property>
         <property name="preferenceCustomization" value="plugin_customization.ini">
         </property>
         <property name="startupForegroundColor" value="000000">
         </property>
         <property name="startupMessageRect" value="7,255,445,20">
         </property>
         <property
               name="cssTheme"
               value="org.eclipse.e4.ui.css.theme.e4_default">
         </property>
         <property
               name="applicationCSSResources"
               value="platform:/plugin/org.eclipse.platform/images/">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.emf.facet.infra.browser.custom.core.registration">
      <browserCustomization
            file="customization/modelexplorer/ApexModelExplorerDefault.uiCustom"
            loadByDefault="true">
      </browserCustomization>
   </extension>
   <extension
         point="org.eclipse.e4.ui.css.swt.theme">
      <theme
            basestylesheeturi="customization/css/modellipse.css"
            id="kr.co.apexsoft.modellipse.perspective.theme.Modellipse"
            label="Modellipse">
      </theme>
      <theme
            basestylesheeturi="customization/css/adwaita.css"
            id="kr.co.apexsoft.modellipse.perspective.theme.adwaita"
            label="adwaita">
      </theme>
      <theme
            basestylesheeturi="customization/css/coffee.css"
            id="kr.co.apexsoft.modellipse.perspective.theme.coffee"
            label="coffee">
      </theme>
      <theme
            basestylesheeturi="customization/css/lime.css"
            id="kr.co.apexsoft.modellipse.perspective.theme.lime"
            label="lime">
      </theme>
      <theme
            basestylesheeturi="customization/css/ocean.css"
            id="kr.co.apexsoft.modellipse.perspective.theme.ocean"
            label="ocean">
      </theme>
   </extension>
   <extension
         point="org.eclipse.emf.facet.infra.query.registration">
      <modelqueryset
            file="customization/modelexplorer/ApexBrowserQuery.querySet">
      </modelqueryset>
   </extension>

</plugin>
